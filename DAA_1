#include <stdio.h>
#include <time.h>
int main() {
 int n, key, i;
 printf("Enter number of medicines: ");
 scanf("%d", &n);
 int a[n];
 for (i = 0; i < n; i++) scanf("%d", &a[i]);
 printf("Enter code to search: ");
 scanf("%d", &key);
 clock_t start = clock();
 for (i = 0; i < n && a[i] != key; i++);
 printf(i < n ? "%d found at %d\n" : "%d not found\n", key, i + 1);
 printf("Time: %f sec\n", (double)(clock() - start) / 
CLOCKS_PER_SEC);
 return 0;
}
