#include<stdio.h>
#include<time.h>
int main() {
 int a[10][10], n, i, j, k;
 double clk;
 clock_t start = clock();
 printf("Enter number of vertices: ");
 scanf("%d", &n);
 printf("Enter the adjacency matrix:\n");
 for(i = 0; i < n; i++)
 for(j = 0; j < n; j++)
 scanf("%d", &a[i][j]);
 for(k = 0; k < n; k++)
 for(i = 0; i < n; i++)
 for(j = 0; j < n; j++)
 a[i][j] = a[i][j] || (a[i][k] && a[k][j]);
 printf("Transitive closure:\n");
 for(i = 0; i < n; i++) {
 for(j = 0; j < n; j++)
 printf("%d ", a[i][j]);
 printf("\n");
 }
 clk = (double)(clock() - start) / CLOCKS_PER_SEC;
 printf("Runtime: %f seconds\n", clk);
 return 0;
}
