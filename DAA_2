#include <stdio.h>
#include <time.h>
int main() {
 int n, i, j, temp;
 printf("Enter number of seats: ");
 scanf("%d", &n);
 int a[n];
 for (i = 0; i < n; i++) scanf("%d", &a[i]);
 clock_t start = clock();
 for (i = 0; i < n-1; i++)
 for (j = 0; j < n-i-1; j++)
 if (a[j] > a[j+1]) {
 temp = a[j];
 a[j] = a[j+1];
 a[j+1] = temp;
 }
 for (i = 0; i < n; i++) printf("%d ", a[i]);
 printf("\nTime: %f sec\n", (double)(clock() - start) / CLOCKS_PER_SEC);
 return 0;
}
